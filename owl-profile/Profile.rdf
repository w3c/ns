    <dc:date>2011-12-06</dc:date>
    <owl:versionIRI rdf:resource="http://www.w3.org/ns/owl-profile/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/owl-profile/Profile">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/owl-profile/DL"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/owl-profile/QL"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/owl-profile/EL"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/owl-profile/RL"/>
    <rdfs:seeAlso rdf:resource="http://www.w3.org/ns/owl-profile/Full"/>
    <dc:description>a unique, top-level Class URI for the concept of OWL 2 Profiles. Instances for this Class represent the OWL 2 Profiles like DL, EL, QL, RL, and Full.</dc:description>
  </rdf:Description>
