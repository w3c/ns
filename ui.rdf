                                    <r:first r:parseType="Resource">
                                        <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                        <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                        <from r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
                                        <label>destination class</label>
                                        <property r:resource="http://www.w3.org/ns/ui#from"/>
                                        <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sequence>
                                    </r:first>
                                    <r:rest r:parseType="Resource">
                                        <r:first r:parseType="Resource">
                                            <r:type r:resource="http://www.w3.org/ns/ui#BooleanField"/>
                                            <label>user can add new</label>
                                            <property r:resource="http://www.w3.org/ns/ui#canMintNew"/>
                                            <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">3</sequence>
                                        </r:first>
                                        <r:rest r:parseType="Resource">
                                            <r:first r:parseType="Resource">
                                                <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                                <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                                <from r:resource="http://www.w3.org/ns/ui#Form"/>
                                                <label>Nested Form (if any)</label>
                                                <optional r:resource="http://www.w3.org/ns/ui#true"/>
                                                <property r:resource="http://www.w3.org/ns/ui#use"/>
                                                <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">4</sequence>
                                                <use r:resource="http://www.w3.org/ns/ui#FormForm"/>
                                            </r:first>
                                            <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                        </r:rest>
                                    </r:rest>
                                </r:rest>
                            </parts>
                        </use>
                    </case>
                    <case r:parseType="Resource">
                        <for r:resource="http://www.w3.org/ns/ui#Classifier"/>
                        <use r:parseType="Resource">
                            <r:type r:resource="http://www.w3.org/ns/ui#Group"/>
                            <parts r:parseType="Resource">
                                <r:first r:parseType="Resource">
                                    <r:type r:resource="http://www.w3.org/ns/ui#Comment"/>
                                    <contents>A classifier allows the user to which classes the item belongs to, given a common superclass of the allowed classes.  Give the superclass here:</contents>
                                    <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sequence>
                                </r:first>
                                <r:rest r:parseType="Resource">
                                    <r:first r:parseType="Resource">
                                        <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                        <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                        <from r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
                                        <label>superclass</label>
                                        <property r:resource="http://www.w3.org/ns/ui#category"/>
                                        <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">4</sequence>
                                    </r:first>
                                    <r:rest r:parseType="Resource">
                                        <r:first r:parseType="Resource">
                                            <r:type r:resource="http://www.w3.org/ns/ui#Comment"/>
                                            <contents>(When the choice is made normally the item is given a r:type. Set this to r:type unless you want the form to set a different property.)</contents>
                                            <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">6</sequence>
                                        </r:first>
                                        <r:rest r:parseType="Resource">
                                            <r:first r:parseType="Resource">
                                                <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                                <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                                <default r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                                                <from r:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
                                                <label>property</label>
                                                <property r:resource="http://www.w3.org/ns/ui#property"/>
                                                <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">8</sequence>
                                            </r:first>
                                            <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                        </r:rest>
                                    </r:rest>
                                </r:rest>
                            </parts>
                        </use>
                    </case>
                    <case r:parseType="Resource">
                        <for r:resource="http://www.w3.org/ns/ui#Multiple"/>
                        <use r:parseType="Resource">
                            <r:type r:resource="http://www.w3.org/ns/ui#Group"/>
                            <parts r:parseType="Resource">
                                <r:first r:parseType="Resource">
                                    <r:type r:resource="http://www.w3.org/ns/ui#BooleanField"/>
                                    <label>ordered</label>
                                    <property r:resource="http://www.w3.org/ns/ui#ordered"/>
                                    <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">0</sequence>
                                </r:first>
                                <r:rest r:parseType="Resource">
                                    <r:first r:parseType="Resource">
                                        <r:type r:resource="http://www.w3.org/ns/ui#IntegerField"/>
                                        <label>minimum number</label>
                                        <property r:resource="http://www.w3.org/ns/ui#min"/>
                                        <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">0</sequence>
                                    </r:first>
                                    <r:rest r:parseType="Resource">
                                        <r:first r:parseType="Resource">
                                            <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                            <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                            <from r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
                                            <label>property</label>
                                            <property r:resource="http://www.w3.org/ns/ui#property"/>
                                            <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sequence>
                                        </r:first>
                                        <r:rest r:parseType="Resource">
                                            <r:first r:parseType="Resource">
                                                <r:type r:resource="http://www.w3.org/ns/ui#Choice"/>
                                                <canMintNew r:resource="http://www.w3.org/ns/ui#true"/>
                                                <from r:resource="http://www.w3.org/ns/ui#Form"/>
                                                <property r:resource="http://www.w3.org/ns/ui#part"/>
                                                <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sequence>
                                                <use r:resource="http://www.w3.org/ns/ui#FieldForm"/>
                                            </r:first>
                                            <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                        </r:rest>
                                    </r:rest>
                                </r:rest>
                            </parts>
                        </use>
                    </case>
                    <case r:parseType="Resource">
                        <for r:resource="http://www.w3.org/ns/ui#Heading"/>
                        <use r:parseType="Resource">
                            <r:type r:resource="http://www.w3.org/ns/ui#SingleLineTextField"/>
                            <property r:resource="http://www.w3.org/ns/ui#contents"/>
                        </use>
                    </case>
                    <case r:parseType="Resource">
                        <for r:resource="http://www.w3.org/ns/ui#Comment"/>
                        <use r:parseType="Resource">
                            <r:type r:resource="http://www.w3.org/ns/ui#MultiLineTextField"/>
                            <property r:resource="http://www.w3.org/ns/ui#contents"/>
                        </use>
                    </case>
                    <dependingOn r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
                    <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">2</sequence>
                </r:first>
                <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </r:rest>
        </parts>
    </Group>

    <Multiple r:about="http://www.w3.org/ns/ui#FieldList">
        <ordered r:resource="http://www.w3.org/ns/ui#true"/>
        <part r:resource="http://www.w3.org/ns/ui#FieldForm"/>
        <property r:resource="http://www.w3.org/ns/ui#parts"/>
        <sequence r:datatype="http://www.w3.org/2001/XMLSchema#integer">10</sequence>
    </Multiple>

    <r:Description r:about="http://www.w3.org/ns/ui#FloatField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#NumericField"/>
    </r:Description>

    <s:Class r:about="http://www.w3.org/ns/ui#Form">
        <s:comment>A form can be any type of single field, or typically a Group of several fields,
    including interspersed headings and comments.  </s:comment>
        <owl:disjointUnionOf r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#ValueField"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#Group"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#Choice"/>
                    <r:rest r:parseType="Resource">
                        <r:first r:resource="http://www.w3.org/ns/ui#Classifier"/>
                        <r:rest r:parseType="Resource">
                            <r:first r:resource="http://www.w3.org/ns/ui#Options"/>
                            <r:rest r:parseType="Resource">
                                <r:first r:resource="http://www.w3.org/ns/ui#Multiple"/>
                                <r:rest r:parseType="Resource">
                                    <r:first r:resource="http://www.w3.org/ns/ui#Heading"/>
                                    <r:rest r:parseType="Resource">
                                        <r:first r:resource="http://www.w3.org/ns/ui#Comment"/>
                                        <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </r:rest>
                                </r:rest>
                            </r:rest>
                        </r:rest>
                    </r:rest>
                </r:rest>
            </r:rest>
        </owl:disjointUnionOf>
        <creationForm r:resource="http://www.w3.org/ns/ui#FormForm"/>
    </s:Class>

    <Form r:about="http://www.w3.org/ns/ui#FormForm">
        <r:type r:resource="http://www.w3.org/ns/ui#Group"/>
        <dc:title>Form for editing Forms</dc:title>
        <parts r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#FF1"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#FF2"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#FF3"/>
                    <r:rest r:parseType="Resource">
                        <r:first r:resource="http://www.w3.org/ns/ui#FieldList"/>
                        <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </r:rest>
                </r:rest>
            </r:rest>
        </parts>
    </Form>

    <r:Description r:about="http://www.w3.org/ns/ui#Group">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Form"/>
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Single"/>
    </r:Description>

    <r:Description r:about="http://www.w3.org/ns/ui#Heading">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Form"/>
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Single"/>
    </r:Description>

    <r:Description r:about="http://www.w3.org/ns/ui#IntegerField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#NumericField"/>
    </r:Description>

    <r:Description r:about="http://www.w3.org/ns/ui#MultiLineTextField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#TextField"/>
    </r:Description>

    <r:Description r:about="http://www.w3.org/ns/ui#Multiple">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Form"/>
    </r:Description>

    <s:Class r:about="http://www.w3.org/ns/ui#NamedNodeURIField">
        <s:comment>A NamedNodeURIField is like a SingleLineTextField, except
		that the value it generates is not a literal string but an RDF node with the given URI.
		Normally users should not see URIs. When they do, this a way to do it.
		</s:comment>
        <s:label>Something by its URI</s:label>
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#TextField"/>
    </s:Class>

    <s:Class r:about="http://www.w3.org/ns/ui#NumericField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#ValueField"/>
        <owl:disjointUnionOf r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#BooleanField"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#TriStateField"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#IntegerField"/>
                    <r:rest r:parseType="Resource">
                        <r:first r:resource="http://www.w3.org/ns/ui#DecimalField"/>
                        <r:rest r:parseType="Resource">
                            <r:first r:resource="http://www.w3.org/ns/ui#FloatField"/>
                            <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        </r:rest>
                    </r:rest>
                </r:rest>
            </r:rest>
        </owl:disjointUnionOf>
    </s:Class>

    <r:Description r:about="http://www.w3.org/ns/ui#Options">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Form"/>
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Single"/>
    </r:Description>

    <r:Description r:about="http://www.w3.org/ns/ui#PhoneField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#ValueField"/>
    </r:Description>

    <s:Class r:about="http://www.w3.org/ns/ui#Single">
        <owl:disjointUnionOf r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#ValueField"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#Group"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#Choice"/>
                    <r:rest r:parseType="Resource">
                        <r:first r:resource="http://www.w3.org/ns/ui#Classifier"/>
                        <r:rest r:parseType="Resource">
                            <r:first r:resource="http://www.w3.org/ns/ui#Options"/>
                            <r:rest r:parseType="Resource">
                                <r:first r:resource="http://www.w3.org/ns/ui#Heading"/>
                                <r:rest r:parseType="Resource">
                                    <r:first r:resource="http://www.w3.org/ns/ui#Comment"/>
                                    <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                </r:rest>
                            </r:rest>
                        </r:rest>
                    </r:rest>
                </r:rest>
            </r:rest>
        </owl:disjointUnionOf>
    </s:Class>

    <r:Description r:about="http://www.w3.org/ns/ui#SingleLineTextField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#TextField"/>
    </r:Description>

    <s:Class r:about="http://www.w3.org/ns/ui#TextField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#ValueField"/>
        <owl:disjointUnionOf r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#SingleLineTextField"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#MultiLineTextField"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#NamedNodeURIField"/>
                    <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </r:rest>
            </r:rest>
        </owl:disjointUnionOf>
    </s:Class>

    <r:Property r:about="http://www.w3.org/ns/ui#TimeField">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Campo de tiempo</s:label>
        <s:label xml:lang="en">Time field</s:label>
    </r:Property>

    <r:Description r:about="http://www.w3.org/ns/ui#TriStateField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#NumericField"/>
    </r:Description>

    <s:Class r:about="http://www.w3.org/ns/ui#ValueField">
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Form"/>
        <s:subClassOf r:resource="http://www.w3.org/ns/ui#Single"/>
        <owl:disjointUnionOf r:parseType="Resource">
            <r:first r:resource="http://www.w3.org/ns/ui#TextField"/>
            <r:rest r:parseType="Resource">
                <r:first r:resource="http://www.w3.org/ns/ui#NumericField"/>
                <r:rest r:parseType="Resource">
                    <r:first r:resource="http://www.w3.org/ns/ui#ColorField"/>
                    <r:rest r:parseType="Resource">
                        <r:first r:resource="http://www.w3.org/ns/ui#DateField"/>
                        <r:rest r:parseType="Resource">
                            <r:first r:resource="http://www.w3.org/ns/ui#DateTimeField"/>
                            <r:rest r:parseType="Resource">
                                <r:first r:resource="http://www.w3.org/ns/ui#PhoneField"/>
                                <r:rest r:parseType="Resource">
                                    <r:first r:resource="http://www.w3.org/ns/ui#EmailField"/>
                                    <r:rest r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                </r:rest>
                            </r:rest>
                        </r:rest>
                    </r:rest>
                </r:rest>
            </r:rest>
        </owl:disjointUnionOf>
    </s:Class>

    <r:Property r:about="http://www.w3.org/ns/ui#annotationForm">
        <s:comment>A form which may be used to add more infromation to an
instance of this class which we know something about.  Anything from
adding just add one more fact, to adding a whole lot of information about a specific
facet of the thing.
</s:comment>
        <s:domain r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <s:label>annotation form</s:label>
        <s:range r:resource="http://www.w3.org/ns/ui#Form"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#backgroundColor">
        <r:type r:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <s:label xml:lang="en">background color</s:label>
        <s:range r:resource="http://www.w3.org/ns/ui#Color"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#backgroundImage">
        <r:type r:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <s:comment>URI or base64 representation of an image</s:comment>
        <s:label xml:lang="en">background image</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#base">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="en">Base</s:label>
        <s:label xml:lang="es">Base</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#category">
        <s:comment>The superclass subclasses of which will be selected.</s:comment>
        <s:domain r:resource="http://www.w3.org/ns/ui#Classifier"/>
        <s:label xml:lang="en">overall superclass</s:label>
        <s:range r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#color">
        <r:type r:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <s:label xml:lang="en">color</s:label>
        <s:range r:resource="http://www.w3.org/ns/ui#Color"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#contents">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Contenido</s:label>
        <s:label xml:lang="en">Contents</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#creationForm">
        <s:comment>A form which may be used to collect information about a
    hitherto locally undocumented instance instance of this class.</s:comment>
        <s:domain r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <s:label>creation form</s:label>
        <s:range r:resource="http://www.w3.org/ns/ui#Form"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#defaultError">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="en">Default error</s:label>
        <s:label xml:lang="es">Error predeterminado</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#dependingOn">
        <s:comment>Many fields prompt for information about a given property of the subject</s:comment>
        <s:domain r:resource="http://www.w3.org/ns/ui#Options"/>
        <s:label xml:lang="en">depending on</s:label>
        <s:range r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#for">
        <s:comment>The value for which this case is selected.</s:comment>
        <s:label xml:lang="en">for</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#from">
        <s:domain r:resource="http://www.w3.org/ns/ui#Choice"/>
        <s:label>from</s:label>
        <s:range r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Class"/>
        <prompt>from what class</prompt>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#initialProperties">
        <s:comment>A really simple way of enabling user interfaces to
            create new information about a class of things is to make a define of properties
            to be specified when a information about a new item
            ("New item" here means an item which the system
            does not have prvious information about yet,
            not an items which has just been created,
            like new friend as opposed to new baby)</s:comment>
        <s:domain r:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
        <s:label>initial properties</s:label>
        <s:range r:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
        <prompt>Properties to be specified for new ones</prompt>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#label">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Etiqueta</s:label>
        <s:label xml:lang="en">Label</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#maxDateOffset">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Fecha máxima de desplazamiento</s:label>
        <s:label xml:lang="en">Maximum date offset</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#maxDatetimeOffset">
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Desplazamiento máximo de fecha y hora</s:label>
        <s:label xml:lang="en">Maximum datetime offset</s:label>
    </r:Property>

    <r:Property r:about="http://www.w3.org/ns/ui#maxLength">
        <s:domain r:resource="http://www.w3.org/ns/ui#TextField"/>
        <s:isDefinedBy r:resource=""/>
        <s:label xml:lang="es">Longitud máxima</s:label>
