        <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/lexicog#LexicographicResource"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/lemon/lexicog#Entry"/>
        <rdfs:comment xml:lang="nl">De relatie &quot;entry&quot; relateert een lexicografisch werk aan een item (of &apos;record&apos; in de literatuur).</rdfs:comment>
        <rdfs:comment xml:lang="es">La propiedad &quot;entry&quot; relaciona un recurso lexicográfico con una entrada (o &apos;registro&apos; en la literatura).</rdfs:comment>
        <rdfs:comment xml:lang="en">The property &quot;entry&quot; relates a lexicographic resource to an entry (or &apos;record&apos; in the literature).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/lemon/lexicog"/>
        <rdfs:label xml:lang="es">entrada</rdfs:label>
        <rdfs:label xml:lang="en">entry</rdfs:label>
        <rdfs:label xml:lang="nl">item</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/lemon/lexicog#restrictedTo -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/lemon/lexicog#restrictedTo">
