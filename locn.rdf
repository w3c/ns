      <rdf:Description rdf:about="https://joinup.ec.europa.eu/system/files/project/Core_Vocabularies-Business_Location_Person-Specification-v0.3_0.zip">
        <dcterms:title>Core Vocabularies Specification</dcterms:title>
      </rdf:Description>
    </wdrs:describedby>
    <dcterms:conformsTo>
